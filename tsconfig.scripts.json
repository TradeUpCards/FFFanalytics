{
    "compilerOptions": {
      "module": "ESNext",
      "target": "ESNext",
      "outDir": "./dist",
      "rootDir": "./src",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": [
      "src/scripts/**/*.ts",
      "src/utils/**/*.ts"
    ]
  }
  